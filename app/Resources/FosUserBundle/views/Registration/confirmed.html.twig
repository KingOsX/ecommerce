{% extends "FOSUserBundle::layout.html.twig" %}

{% trans_default_domain 'FOSUserBundle' %}

{% block fos_user_content %}

<div class="col-md-6 col-md-offset-3">
    <div class="well">

    <p style="text-align: center;font-size: 16px">{{ 'registration.confirmed'|trans({'%username%': user.username}) }}</p>
    {% if targetUrl %}
    <p><a href="{{ targetUrl }}">{{ 'registration.back'|trans }}</a></p>
    {% endif %}
    <h5 style="text-align: center;font-size: 16px">Pour finaliser votre inscription veuillez remplir votre profil</h5>


</div>
    <a class="btn btn-primary pull-left" href="{{ path('vapamax_main_homepage') }}">Plus tard</a>
    <a class="btn btn-success pull-right" href="{{ path('fos_user_profile_edit') }}">Mon Compte</a>
</div>

{% endblock fos_user_content %}
